<?xml version="1.0"?>
<launch>
  <rosparam param="use_sim_time">"True"</rosparam>
  <node pkg="tf" type="static_transform_publisher" name="san_tf"
        args="0 0 0.5 0 0 0 world scan 100" />
  <node name="model_loader" pkg="model_loader" type="scan_reader"/>
  <node name="rviz_model_loader" pkg="rviz" type="rviz" respawn="false" output="screen" args="-d $(find model_loader)/launch/test.rviz" required="true"/>
</launch>
